{
  "name": "creact-boilerplate",
  "description": "Legacy React + TypeScript boilerplate with Create React App (Webpack).",
  "keywords": [
    "CRA",
    "SCSS",
    "Bootstrap",
    "TypeScript",
    "React"
  ],
  "author": "Coding Sable",
  "license": "ISC",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "creact-boilerplate"
  },
  "main": "index.tsx",
  "engines": {
    "yarn": ">= 1.22.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run jest"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "echo": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "jest": "jest",
    "prepare": "husky install",
    "json": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.70",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/eslint": "^8.56.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/sass-loader": "^8.0.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "autoprefixer": "10.4.16",
    "bootstrap": "5.3.2",
    "concurrently": "8.2.2",
    "cssnano": "6.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jsdom": "23.2.0",
    "json-server": "1.0.0-alpha.21",
    "lodash": "4.17.21",
    "postcss": "8.4.33",
    "postcss-preset-env": "9.3.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.2",
    "react-bootstrap": "2.9.2",
    "react-redux": "9.1.0",
    "react-router": "6.21.2",
    "react-router-dom": "6.21.2",
    "redux": "5.0.1",
    "sass": "1.69.7",
    "sass-loader": "13.3.3",
    "uuid": "9.0.1"
  }
}
